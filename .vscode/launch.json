{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Next.js: Launch",
        "program": "${workspaceFolder}/node_modules/next/dist/bin/next",
        "args": ["dev"],
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "node",
        "env": {
          "NODE_ENV": "development"
        },
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "outputCapture": "std"
      },
      {
        "type": "node",
        "request": "attach",
        "name": "Next.js: Attach",
        "port": 9229,
        "restart": true,
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }
  